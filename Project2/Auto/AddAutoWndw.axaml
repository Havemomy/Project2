<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Project2.Auto.AddAutoWndw"
        Title="AddAutoWndw">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <TextBlock Text="Марка" HorizontalAlignment="Center" FontWeight="DemiBold"/>
                <TextBox Width="150" Name="MarkaTxt"/>
                <TextBlock Text="Номер" HorizontalAlignment="Center" FontWeight="DemiBold"/>
                <TextBox Width="150" Name="NomerTxt"/>
                <TextBlock Text="Расход" HorizontalAlignment="Center" FontWeight="DemiBold"/>
                <TextBox Width="150" Name="RashodTxt"/>
                <Button Name="AddBtn" Content="Добавить" Background="Lavender" HorizontalAlignment="Center" Margin="0 5 0 0" Click="AddBtn_OnClick" />
        </StackPanel>
</Window>
